{
  "name": "minimal-react-starter",
  "version": "0.0.3",
  "description": "Minimal react starter project",
  "main": "src/index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "better-npm-run dev",
    "build": "better-npm-run build",
    "prod": "better-npm-run prod"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon server.js --ignore src --ignore node_modules",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "webpack -p --config webpack.prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "prod": {
      "command": "node server.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "keywords": [
    "react",
    "webpack",
    "babel",
    "minimal",
    "starter"
  ],
  "author": "Nemmar Ramos",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/nemmarramos/react-starter-lite.git"
  },
  "dependencies": {
    "better-npm-run": "0.0.15",
    "express": "^4.15.2",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-hot-loader": "^3.0.0-beta.6"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "nodemon": "^1.11.0",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  }
}
